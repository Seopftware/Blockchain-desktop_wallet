{
  "name": "Tripcoin-Wallet",
  "productName": "Tripcoin Wallet",
  "scripts": {
    "start": "concurrently --kill-others \" yarn run startReact \" \" cross-env ENV=dev electron .\"",
    "startReact": "cd uidev && npm start",
    "build": "electron-packager . --all --prune=false --icon=icon.ico --ignore=/uidev --overwrite --out=dist",
    "prebuild": "yarn run deleteWallet && cd uidev && npm run build",
    "deleteWallet": "del \"blockchain-pow/src/privateKey\""
  },
  "main": "main.js",
  "dependencies": {
    "axios": "^0.18.0",
    "concurrently": "^4.0.1",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "electron": "^3.0.2",
    "electron-packager": "^12.1.2",
    "get-port": "^4.0.0",
    "styled-components": "^3.4.9",
    "typography": "^0.16.17"
  }
}
